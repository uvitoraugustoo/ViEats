<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity"
    android:background="#FFFFFF"
>

    <EditText
        android:id="@+id/editTextText4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:hint="Digite seu CPF"
        android:inputType="text"
        android:shadowColor="#132E5D"
        android:textColor="#132E5D"
        android:textColorHighlight="#132E5D"
        android:textColorHint="#4B4545"
        tools:layout_editor_absoluteX="100dp"
        tools:layout_editor_absoluteY="531dp" />

    <EditText
        android:id="@+id/editTextText3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:hint="Digite sua senha"
        android:inputType="text"
        android:shadowColor="#132E5D"
        android:textColor="#132E5D"
        android:textColorHighlight="#132E5D"
        android:textColorHint="#4B4545"
        tools:layout_editor_absoluteX="100dp"
        tools:layout_editor_absoluteY="464dp" />

    <TextView
        android:id="@+id/cadas"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="CADASTRE-SE"
        android:textColor="#132E5D"
        android:textSize="34sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.476"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.37" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="195dp"
        android:layout_height="160dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.145"
        app:srcCompat="@drawable/vieats" />

    <EditText
        android:id="@+id/editTextText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:hint="Digite seu email"
        android:inputType="text"
        android:shadowColor="#132E5D"
        android:textColor="#132E5D"
        android:textColorHighlight="#132E5D"
        android:textColorHint="#4B4545"
        tools:layout_editor_absoluteX="100dp"
        tools:layout_editor_absoluteY="397dp" />

    <EditText
        android:id="@+id/editTextText2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:hint="Digite seu nome"
        android:inputType="text"
        android:shadowColor="#132E5D"
        android:textColor="#132E5D"
        android:textColorHighlight="#132E5D"
        android:textColorHint="#4B4545"
        tools:layout_editor_absoluteX="100dp"
        tools:layout_editor_absoluteY="327dp" />

    <Button
        android:id="@+id/realizarC"
        android:layout_width="291dp"
        android:layout_height="58dp"
        android:backgroundTint="#132E5D"
        android:text="CADASTRAR"
        tools:layout_editor_absoluteX="60dp"
        tools:layout_editor_absoluteY="621dp" />
</androidx.constraintlayout.widget.ConstraintLayout>